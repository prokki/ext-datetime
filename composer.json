{
    "name": "prokki/ext-datetime",
    "type": "library",
    "description": "Extends the common php DateTime objects with a lot of additional helpful methods.",
    "keywords": [
        "datetime",
        "immutable",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Falko Matthies",
            "email": "falko.m@web.de"
        }
    ],
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "ExtDateTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExtDateTime\\Tests\\": "tests/"
        }
    }
}
